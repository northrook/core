{
  "name": "northrook/core",
  "description": "Core implementation.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Martin Nielsen",
      "email": "mn@northrook.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Core\\": "src",
      "Support\\": "support"
    },
    "files": [
      "functions/arguments.php",
      "functions/array.php",
      "functions/core.php",
      "functions/filesystem.php",
      "functions/normalize.php",
      "functions/string.php",
      "support/constants.php",
      "support/functions.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.3",
    "ext-ctype": "*",
    "ext-curl": "*"
  },
  "require-dev": {
    "northrook/dev": "dev-main",
    "northrook/pathfinder": "dev-main",
    "voku/portable-ascii": "*"
  },
  "scripts": {
    "phpstan": "vendor/bin/phpstan analyse"
  }
}
